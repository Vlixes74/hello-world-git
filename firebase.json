{
  "apphosting": {
    "backendId": "staged-qr-loader",
    "rootDir": "./",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      ".history",
      "functions",
      "/src/uploads/*"
    ]
  },
  "emulators": {
    "apphosting": {
      "port": 3000,
      "rootDirectory": "./src",
      "startCommand": "npm run Stardev"
    },
    "ui": {
      "enabled": true,
      "port": 4999
    }
  }  
}
